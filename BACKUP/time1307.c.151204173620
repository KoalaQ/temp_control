#define  SCL_H  PORTD|=1<<PD0 //
#define  SCL_L  PORTD&=~(1<<PD0) //
#define  SDA_IN   PORTD&(~(1<<PD1)) //会获得PB1的值，判断是否等于0来确定值
#define  SDA_H   PORTD|=1<<PD1 //
#define  SDA_L  PORTD&=~(1<<PD1)  //

void time1307_init(void){
   DDRD |= 0x03;//先全部输出
}
void I2C_start(void){
   SDA_H;
   SCL_H;
   delay_us(8);
   SDA_L;
   delay_us(8);
   SDA_L;
   
}


